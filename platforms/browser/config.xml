<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.wifimax.app" version="1.0.16" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>Wifimax</name>
    <description>
        application for wifimax data analysis
    </description>
    <author email="suporte@wifimax.com.br" href="http://wifimax.com">
        Wifimax Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="deployment-target" value="7.1.2" />
    <preference name="fullscreen" value="true" />
    <preference name="phonegap-version" value="cli-7.1.0" />
</widget>

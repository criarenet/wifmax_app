    <platform name="wp8">
        <icon height="99" platform="wp8" src="/ApplicationIcon.png" width="99" />
        <icon height="159" platform="wp8" src="/Background.png" width="159" />
        <splash height="1280" platform="wp8" src="/screen-portrait.jpg" width="768" />

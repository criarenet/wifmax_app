<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.wifimax.app" version="1.0.14" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Wifimax</name>
    <description>
        application for wifimax data analysis
    </description>
    <author email="suporte@wifimax.com.br" href="http://wifimax.com">
        Wifimax Team
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="deployment-target" value="7.1.2" />
    <preference name="fullscreen" value="true" />
    <platform name="android">
        <icon density="ldpi" src="/drawable-ldpi-icon.png" />
        <icon density="mdpi" src="/drawable-mdpi-icon.png" />
        <icon density="hdpi" src="/drawable-hdpi-icon.png" />
        <icon density="xhdpi" src="/drawable-xhdpi-icon.png" />
        <icon density="xxhdpi" src="/drawable-xxhdpi-icon.png" />
        <icon density="xxxhdpi" src="/drawable-xxxhdpi-icon.png" />
        <splash density="land-ldpi" src="/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <icon height="57" platform="ios" src="/icon.png" width="57" />
        <icon height="114" platform="ios" src="/icon@2x.png" width="114" />
        <icon height="40" platform="ios" src="/icon-40.png" width="40" />
        <icon height="80" platform="ios" src="/icon-40@2x.png" width="80" />
        <icon height="50" platform="ios" src="/icon-50.png" width="50" />
        <icon height="100" platform="ios" src="/icon-50@2x.png" width="100" />
        <icon height="60" platform="ios" src="/icon-60.png" width="60" />
        <icon height="120" platform="ios" src="/icon-60@2x.png" width="120" />
        <icon height="180" platform="ios" src="/icon-60@3x.png" width="180" />
        <icon height="72" platform="ios" src="/icon-72.png" width="72" />
        <icon height="144" platform="ios" src="/icon-72@2x.png" width="144" />
